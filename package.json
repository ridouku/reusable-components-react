    "lint:duplicate": "jscpd --ignore ./src",
    "lint:fix": "npm run lint:check --fix",
    "lint:format": "prettier --write \"src/**/*.ts?(x)\" \"*.json\"",
    "depcheck": "depcheck . --ignores=\"@types/*,react,react-dom,husky,style-loader,pretty-quick,identity-obj-proxy,gulp-typescript,gulp-exec,eslint-plugin-prettier,eslint-config-ts-react-important-stuff,eslint-config-prettier,enzyme-adapter-react-16,enzyme,del,css-loader,copyfiles,concurrently,barrel-me,@testing-library/user-event,@babel/runtime,@babel/preset-typescript,@babel/preset-react,@babel/preset-env,@babel/polyfill,@babel/plugin-transform-runtime,lodash,clsx,prettier,eslint-config-react-app,babel-eslint,eslint-plugin-import,eslint-plugin-flowtype,eslint-plugin-jsx-a11y\"",
    "validate": "npm run depcheck && npm run lint && npm run test"
  },
