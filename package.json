{
  "name": "reusable-components-react-demo",
  "version": "1.0.0",
  "scripts": {
    "build": "gulp build-components",
    "publish-cmp": "npm publish lib/src/components",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
